@model IEnumerable<MasonryQuantities.WallInformation>

@{
    ViewBag.Title = "All wall information";
}

<h2>View all the walls in the database</h2>

<p>
   
</p>
<table>
    <tr>
        <th>
            WallHeight
        </th>
        <th>
            Walllength
        </th>
        <th>
            BlockType
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WallHeight)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Walllength)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BlockType)
        </td>
        
    </tr>
}

</table>
